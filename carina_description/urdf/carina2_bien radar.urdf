    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <joint name="bumblebee_radaresr_joint" type="fixed">
   <!-- <origin xyz="2.05 0 0.40" rpy="0 0 0" />  -->
     <!-- <origin xyz="2.05 0 0.35" rpy="-1.73 0  -2.00 " /> -->
     <origin xyz="-1.0 1.0 2.35 " rpy="1.23 -2.00  0.3" />
      <parent link="bumblebee"/>
    <child link="radaresr"/>  
  </joint>

  <link name="radaresr">
    <inertial>
      <mass value="2" />
      <origin xyz="0 0 0" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>
    
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.0492 0.132 0.0902"/>
      </geometry>
      <material name="Black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
  </link>



</robot>
