      <geometry>
        <box size="0.0474 0.157 0.036"/>
      </geometry>
      <material name="Gold">
        <color rgba="0.878 0.690 0.356 1"/>
      </material>
    </visual>
  </link>

  <joint name="base_link_imu_joint" type="fixed">
    <origin xyz="-1.31 0 1.20" rpy="0 0 0" />
    <parent link="base_link"/>
    <child link="imu_link"/>  
